{
  "extends": "@istanbuljs/nyc-config-typescript",
  "require": [
    "ts-node/register",
    "source-map-support/register"
  ],
  "include": ["src"],
  "exclude": ["examples", "dist"],
  "cache": false,
  "sourceMap": true,
  "all": true,
  "instrument": true,
  "reporter": ["lcov", "text", "text-summary"]
}
